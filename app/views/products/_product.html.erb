<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= product.category_id %>
  </p>

  <p>
    <strong>Subcategory:</strong>
    <%= product.subcategory_id %>
  </p>

  <p>
    <strong>Marca:</strong>
    <%= product.marca_id %>
  </p>

  <p>
    <strong>Tech:</strong>
    <%= product.tech_id %>
  </p>

</div>
